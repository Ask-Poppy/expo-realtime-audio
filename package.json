  "repository": {
    "type": "git",
    "url": "git+https://github.com/AskPoppy/expo-realtime-audio.git"
  },
  "bugs": {
    "url": "https://github.com/AskPoppy/expo-realtime-audio/issues"
  },
  "homepage": "https://github.com/AskPoppy/expo-realtime-audio#readme",
  "keywords": [
    "expo",
    "react-native",
    "audio",
    "streaming",
    "realtime",
    "microphone",
    "recording",
    "playback",
    "voice",
    "speech",
    "ios",
    "avfoundation",
    "avaudioengine"
